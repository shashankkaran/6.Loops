using System;

class NumbersNotDivisibleBy3And7
{
    static void Main()
    {
       printf("Enter an integer:");
        int n ;
        scanf(%d ,&n);
        for (int i = 1; i <=n; i++)
        {
            if (i%3!=0&&i%7!=0)
            {
                printf(" the number %d is divisible ",n);
            }
            else{
            printf("not divisible");
            }
        }
      
    }
}

